class playlist():
    def __init__(self,sarki_listesi):
        self.sarki_listesi=sarki_listesi
        self.index=-1
        def __iter__(self):
            return self
            def __next__(self):
                self.index+=1
                if self.index<len(self.sarki_listesi):
                    return self.sarki_listesi[self.index]
